{
  "name": "swipe4paws",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && rimraf ./dist && ng build",
    "build:backend": "rimraf ./backend/dist && tsc -p backend/tsconfig.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:swipe4paws": "node dist/swipe4paws/server/server.mjs",
    "dev": "ts-node-dev --poll ./src/index.ts",
    "start:backend": "npm run build:backend && node backend/dist/index.js",
    "dev:backend": "nodemon backend/src/index.js"
  },
  "private": true,
  "dependencies": {
